{
  "name": "squareonewp/acf-field-types",
  "type": "library",
  "description": "Provides custom WP field types for the theme.",
  "require": {
    "php": ">=7.4"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "SquareOne\\Acf\\Fields\\": "src/"
    }
  },
  "scripts": {
    "lint": [
      "phpcs --ignore=vendor --exclude=PSR1.Methods.CamelCapsMethodName --extensions=php --standard=PSR12 ."
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
